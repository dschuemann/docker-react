    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "docker-react",
        "image": "dschuemann/docker-react",
        "hostname": "docker-react",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "memory": 128
      }
    ]
  }
  