{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "multi-docker-react",
        "image": "dschuemann/multi-docker-react",
        "hostname": "multi-docker-react",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "memory": 128
      }
    ]
  }
  